{
    "name": "vscode-powerquery-server",
    "version": "0.0.31",
    "description": "Power Query language server implementation.",
    "author": "Microsoft Corporation",
    "license": "MIT",
    "homepage": "https://github.com/microsoft/vscode-powerquery#readme",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/microsoft/vscode-powerquery.git",
        "directory": "server"
    },
    "issues": {
        "url": "https://github.com/microsoft/vscode-powerquery/issues"
    },
    "scripts": {
        "init": "npm install",
        "check": ".\\node_modules\\.bin\\prettier --config \".prettierrc\" --list-different src\\**\\*.ts",
        "build": ".\\node_modules\\.bin\\tsc",
        "watch": ".\\node_modules\\.bin\\tsc -watch -p",
        "test": "mocha --reporter mocha-multi-reporters --reporter-options configFile=src/test/mochaConfig.json -r ts-node/register src/test/**/*.ts",
        "webpack-prod": "node_modules\\.bin\\webpack --mode production",
        "webpack-dev": "node_modules\\.bin\\webpack  --mode development --watch"
    },
    "main": "lib\\server.d.ts",
    "types": "lib\\server.d.ts",
    "engines": {
        "node": "*"
    },
    "dependencies": {
        "@microsoft/powerquery-parser": "0.1.48",
        "@microsoft/powerquery-language-services": "0.1.1",
        "vscode-languageserver": "6.0.0-next.7",
        "vscode-languageserver-textdocument": "1.0.0-next.5"
    },
    "devDependencies": {
        "@types/node": "12.12.2",
        "typescript": "3.8.3",
        "webpack": "4.42.1",
        "webpack-cli": "3.3.11"
    }
}
