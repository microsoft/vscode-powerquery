{
    "name": "vscode-powerquery-server",
    "description": "Power Query language server implementation.",
    "version": "0.0.31",
    "author": "Microsoft Corporation",
    "license": "MIT",
    "engines": {
        "node": "*"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/microsoft/vscode-powerquery/"
    },
    "prettier": {
        "trailingComma": "all",
        "tabWidth": 4,
        "semi": true,
        "singleQuote": false,
        "printWidth": 120
    },
    "scripts": {
        "build": "node_modules\\.bin\\tsc -p ./",
        "watch": "node_modules\\.bin\\tsc -watch -p ./",
        "webpack-prod": "node_modules\\.bin\\webpack --mode production",
        "webpack-dev": "node_modules\\.bin\\webpack  --mode development --watch"
    },
    "dependencies": {
        "@microsoft/powerquery-format": "file:../packages/powerquery-format",
        "@microsoft/powerquery-language-services": "file:../packages/powerquery-language-services",
        "@microsoft/powerquery-library": "file:../packages/powerquery-library",
        "@microsoft/powerquery-parser": "0.1.36",
        "vscode-languageserver": "6.0.0-next.7",
        "vscode-languageserver-textdocument": "1.0.0-next.5"
    }
}
