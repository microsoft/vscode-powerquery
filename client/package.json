{
    "name": "vscode-powerquery-client",
    "description": "VS Code part of language server",
    "author": "Microsoft Corporation",
    "license": "MIT",
    "version": "0.0.31",
    "publisher": "powerquery",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/microsoft/vscode-powerquery.git",
        "directory": "client"
    },
    "prettier": {
        "trailingComma": "all",
        "tabWidth": 4,
        "semi": true,
        "singleQuote": false,
        "printWidth": 120
    },
    "engines": {
        "vscode": "1.40.0"
    },
    "scripts": {
        "build": "..\\node_modules\\.bin\\tsc -p ./",
        "watch": "..\\node_modules\\.bin\\tsc -watch -p ./",
        "pretest": "npm run build",
        "test-disabled": "node ./out/test/runTest.js",
        "webpack-prod": "..\\node_modules\\.bin\\webpack --mode production",
        "webpack-dev": "..\\node_modules\\.bin\\webpack  --mode development --watch"
    },
    "dependencies": {
        "vscode-languageclient": "6.0.0-next.8"
    }
}
